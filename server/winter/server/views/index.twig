                                    <a href="#"> <i class="fas fa-star"></i> </a>
                                </div>
                                <h5>{{article.price}} DN</h5>
                                <div class="social_icon">
                                    <a href="#"><i class="ti-heart"></i></a>
                                    <a href="#"><i class="ti-bag"></i></a>
                                </div>
                                  <h5>51794635</h5>
                            </div>
                        </div>
					{% endfor %}
                    </div>
                </div> 
            </div>
        </div>
